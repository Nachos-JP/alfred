  },
  "homepage": "https://github.com/Nachos-JP/gekko#readme",
  "devDependencies": {
    "css-loader": "^2.1.1",
    "electron": "^4.0.6",
    "electron-packager": "^13.1.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
