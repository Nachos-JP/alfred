{
  "name": "gekko",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "electron src/",
    "dev": "node-sass src/style/scss --output src/style --output-style expanded && electron src/",
    "package:linux": "electron-packager src/ GEKKO --overwrite --platform=linux --arch=x64 --out=dist",
    "package:mac": "electron-packager src/ GEKKO --overwrite --platform=darwin --arch=x64 --out=dist",
    "package:win": "electron-packager src/ GEKKO --overwrite --platform=win32 --arch=x64 --out=dist",
    "package:all": "electron-packager src/ GEKKO --overwrite --platform=all --arch=x64 --out=dist",
    "sass": "node-sass src/style/scss --output src/style --output-style expanded"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nachos-JP/gekko.git"
  },
  "author": "Shota Maekawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nachos-JP/gekko/issues"
  },
  "homepage": "https://github.com/Nachos-JP/gekko#readme",
  "devDependencies": {
    "electron": "^4.0.6",
    "electron-packager": "^13.1.0",
    "node-sass": "^4.11.0"
  },
  "private": true
}
